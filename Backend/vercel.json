{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/Public(.*)",
            "dest": "/Public/"
        }
    ]
}

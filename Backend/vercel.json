{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "serve"
        }
    ],
    "routes": [
        {
            "src": "/Public/(.*)",
            "dest": "/Public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}

{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/aboutimg",
            "dest": "/Public/Images/Login-Bg.png"
        },
        {
            "src": "/Public/(.*)",
            "dest": "/Public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}

{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/Public/(.*)",
            "dest": "/Public/$1"
        },
        {
            "src": "/about",
            "dest": "/Views/about.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}

{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/Public(.*)",
            "destination": "/Public/"
        },
        {
            "source": "/api/v1(.*)",
            "destination": "/index.js"
        }
    ]
}

{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/Public/(.*)",
            "dest": "/Backend/Public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/Backend/index.js"
        }
    ]
}

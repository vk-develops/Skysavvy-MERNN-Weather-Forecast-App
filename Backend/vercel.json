{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/v1/(.*)",
            "destination": "/Api/index.js"
        },
        {
            "source": "/Public/(.*)",
            "destination": "/Public/"
        }
    ]
}

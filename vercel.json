{
    "builds": [
        {
            "src": "/Backend/*.js",
            "use": "@vercel/node"
        }
    ],

    "rewrites": [
        {
            "source": "/api/v1/(.*)",
            "destination": "/Backend/index.js"
        },
        {
            "source": "/Public/(.*)",
            "destination": "/Backend/Public/"
        }
    ]
}
